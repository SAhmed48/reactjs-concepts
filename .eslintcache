State([]); // array using usestate\n\n    const addItems = () => {\n        setItem([...items, {id: items.length, value: Math.floor(Math.random() * 10) }])\n    }\n\n    const decrementCounter = () => {\n        if(count > 0){\n            setCounter(prevCount => prevCount - 1); // use prevCount by pass function to setCount to update state.\n        }\n    }\n\n    return (\n        <div>\n            Count {count}\n            <br />\n            <button onClick={() => setCounter(initialCount)}>Reset Counter</button>\n            <br />\n            <button onClick={() => setCounter(prevCount => prevCount + 1)}>Increment Count</button>\n            <br />\n            <button onClick={() => decrementCounter()}>Decrement Count</button>\n\n            <br />\n            <br />\n            <br />\n            <br />\n            <label>First Name:</label>\n            <input type='text' value={name.firstname} onChange={e => setName({...name, firstname: e.target.value })} />\n            <br />\n            <br />\n            <label>Last Name:</label>\n            <input type='text' value={name.lastname} onChange={e => setName({...name, lastname: e.target.value })} />\n\n            <h3>First Name: {name.firstname}</h3> ------- <h3>Last Name: {name.lastname}</h3>\n\n            <button onClick={() => addItems()}>Add Items</button>\n            <br />\n            <ul>\n                {items.map(item => (\n                    <li>{item.id, item.value}</li>\n                ))}\n            </ul>\n        </div>\n    )\n}\n\nexport default UseStateHook;\n","/Users/apple/Documents/reactJS/reactjs-concepts/src/Hooks/useEffectHook.js",[],"/Users/apple/Documents/reactJS/reactjs-concepts/src/Hooks/useEffectDataFetch.js",[],"/Users/apple/Documents/reactJS/reactjs-concepts/src/Hooks/useContext.js",[],"/Users/apple/Documents/reactJS/reactjs-concepts/src/Hooks/useReducer.js",["56"],"import React, {useReducer} from 'react'\n\nconst initialState = 0;\nconst reducer = (state, action) => {\n    switch(action){\n        case 'Increment':\n            return state + 1;\n        case 'Decrement':\n            return state - 1;\n        case 'reset':\n            return initialState;        \n    }\n}\n\nfunction CounterReducer() {\n    const [state, dispatch] = useReducer(reducer, initialState);\n\n    return (\n        <div>\n            <h5>Count: {state}</h5>\n            <button onClick={() => {dispatch('Increment')}}>Increment</button>\n            <button onClick={() => {dispatch('Decrement')}}>Increment</button>\n            <button onClick={() => {dispatch('reset')}}>Increment</button>\n        </div>\n    )\n}\n\nexport default CounterReducer\n","/Users/apple/Documents/reactJS/reactjs-concepts/src/Hooks/useCallback/main.js",["57","58"],"/Users/apple/Documents/reactJS/reactjs-concepts/src/Hooks/useCallback/button.js",[],"/Users/apple/Documents/reactJS/reactjs-concepts/src/Hooks/useCallback/display.js",[],"/Users/apple/Documents/reactJS/reactjs-concepts/src/Hooks/useCallback/title.js",[],{"ruleId":"59","replacedBy":"60"},{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","severity":1,"message":"64","line":48,"column":33,"nodeType":"65","messageId":"66","endLine":48,"endColumn":34},{"ruleId":"67","severity":1,"message":"68","line":5,"column":5,"nodeType":"69","messageId":"70","endLine":12,"endColumn":6},{"ruleId":"71","severity":1,"message":"72","line":13,"column":7,"nodeType":"73","endLine":13,"endColumn":12,"suggestions":"74"},{"ruleId":"71","severity":1,"message":"75","line":17,"column":8,"nodeType":"73","endLine":17,"endColumn":16,"suggestions":"76"},"no-native-reassign",["77"],"no-negated-in-lhs",["78"],"no-sequences","Unexpected use of comma operator.","SequenceExpression","unexpectedCommaExpression","default-case","Expected a default case.","SwitchStatement","missingDefaultCase","react-hooks/exhaustive-deps","React Hook useCallback has an unnecessary dependency: 'age'. Either exclude it or remove the dependency array.","ArrayExpress